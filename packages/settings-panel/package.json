    "@notion-kit/tsconfig": "workspace:*",
    "@types/file-saver": "^2.0.7",
    "@types/react": "catalog:react19",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "react": "catalog:react19",
    "tsup": "catalog:",
    "typescript": "catalog:"
