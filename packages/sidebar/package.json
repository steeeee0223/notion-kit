{
  "name": "@notion-kit/sidebar",
  "version": "0.14.0",
  "type": "module",
  "bugs": {
    "url": "https://github.com/steeeee0223/notion-kit/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/steeeee0223/notion-kit.git",
    "directory": "packages/sidebar"
  },
  "exports": {
    ".": {
      "types": "./dist/core/index.d.mts",
      "default": "./src/core/index.ts"
    },
    "./presets": {
      "types": "./dist/presets/index.d.mts",
      "default": "./src/presets/index.ts"
    }
  },
  "files": [
    "dist"
  ],
  "main": "./dist/core/index.mjs",
  "module": "./dist/core/index.mjs",
  "types": "./dist/core/index.d.mts",
  "license": "MIT",
  "scripts": {
    "build": "tsdown",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "dev": "tsdown --watch",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false"
  },
  "dependencies": {
    "@dnd-kit/core": "catalog:ui",
    "@dnd-kit/modifiers": "catalog:ui",
    "@dnd-kit/sortable": "catalog:ui",
    "@notion-kit/cn": "workspace:*",
    "@notion-kit/hooks": "workspace:*",
    "@notion-kit/icon-block": "workspace:*",
    "@notion-kit/icon-menu": "workspace:*",
    "@notion-kit/icons": "workspace:*",
    "@notion-kit/schemas": "workspace:*",
    "@notion-kit/shadcn": "workspace:*",
    "@notion-kit/spinner": "workspace:*",
    "@notion-kit/tree-2": "workspace:*",
    "@notion-kit/utils": "workspace:*"
  },
  "devDependencies": {
    "@notion-kit/eslint-config": "workspace:*",
    "@notion-kit/prettier-config": "workspace:*",
    "@notion-kit/tsconfig": "workspace:*",
    "babel-plugin-react-compiler": "catalog:react-compiler",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "react-compiler-runtime": "catalog:react-compiler",
    "tsdown": "catalog:",
    "typescript": "catalog:"
  },
  "peerDependencies": {
    "react": "catalog:react19",
    "react-dom": "catalog:react19"
  },
  "prettier": "@notion-kit/prettier-config",
  "babel": {
    "plugins": [
      "babel-plugin-react-compiler"
    ]
  }
}
